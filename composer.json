{
    "name": "ophelios/php-ethereum-moralis",
    "description": "Simple token price fetching from Moralis.io for PHP.",
    "type": "library",
    "license": "MIT",
    "autoload": {
      "psr-4": {
        "Moralis\\": "src/Moralis"
      }
    },
    "authors": [
      {
        "name": "David Tucker",
        "email": "david.tucker@ophelios.com"
      }
    ],
    "require": {
      "php": ">=8.4",
      "guzzlehttp/guzzle": "^7.6.1",
      "ophelios/php-apcu-cache": "^1.0.0"
    },
    "require-dev": {
      "phpunit/phpunit": "^10.5",
      "vlucas/phpdotenv": "^5.6"
    },
    "scripts": {
      "test": "XDEBUG_MODE=coverage vendor/bin/phpunit",
      "test-coverage": "XDEBUG_MODE=coverage vendor/bin/phpunit --coverage-clover clover.xml"
    },
    "minimum-stability": "dev"
}
